{
	"name": "directus-extension-sql-query-panel",
	"version": "1.0.1",
	"type": "module",
	"description": "Insights Panel for viewing results for dynamic sql queries",
	"icon": "extension",
	"author": {
		"name": "Gerard Lamusse"
	},
	"files": [
		"dist",
		"README.md"
	],
	"directus:extension": {
		"host": "^10.1.14",
		"type": "bundle",
		"path": {
			"app": "dist/app.js",
			"api": "dist/api.js"
		},
		"entries": [
			{
				"type": "endpoint",
				"name": "insights",
				"source": "src/insights/index.ts"
			},
			{
				"type": "panel",
				"name": "sql-query",
				"source": "src/sql-query/index.ts"
			}
		]
	},
	"keywords": [
		"directus",
		"directus-extension",
		"directus-custom-bundle"
	],
	"scripts": {
		"build": "directus-extension build",
		"dev": "directus-extension build -w --no-minify",
		"link": "directus-extension link",
		"add": "directus-extension add"
	},
	"devDependencies": {
		"@directus/extensions": "^0.1.1",
		"@directus/extensions-sdk": "10.1.14",
		"@types/node": "^20.8.10",
		"typescript": "^5.2.2",
		"vue": "^3.3.7"
	},
	"dependencies": {
		"@directus/types": "^11.0.1",
		"lodash.debounce": "^4.0.8"
	}
}
